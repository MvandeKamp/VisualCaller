<Application x:Class="VisualCaller.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:VisualCaller"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <SolidColorBrush x:Key="UiLightGray" Color="#FF36393F"/>
        <SolidColorBrush x:Key="UiMediumGray" Color="#FF2F3136"/>
        <SolidColorBrush x:Key="UiMainColor" Color="#FF8A00DB"/>
        <SolidColorBrush x:Key="UiDarkGray" Color="#FF202225"/>
        <SolidColorBrush x:Key="SelectionBrush" Color="#667C1685"/>
    </Application.Resources>
</Application>
